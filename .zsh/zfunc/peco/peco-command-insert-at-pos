# peco-command-insert-at-pos() {
  local pos=$CURSOR
  local selected="$(for f in $commands; do echo $f; done | peco)"
  if [ -n "$selected" ]; then
    BUFFER="${BUFFER[1,$pos]}$(echo $selected | sed 's/ /\\ /g' | tr '[:cntrl:]' ' ')${BUFFER[$pos,-1]}"
    CURSOR=$#BUFFER
  fi
  zle -R
# }
# zle -N peco-command-insert-at-pos
