# peco-homebrew-cask-open-appdir() {
  local selected="$(find /opt/homebrew-cask/Caskroom -type d -depth 2 -not -name '.metadata' | peco --query="$LBUFFER")"
  if [ -n "$selected" ]; then
    BUFFER="open ${selected}"
    zle accept-line
  fi
  zle -R
# }
