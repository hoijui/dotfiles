# vim:fdm=marker sw=2 ts=2 ft=snippet noexpandtab:

# tmuxinator
snippet layout_mainv
      layout: main-vertical
      panes:
        - $1

snippet layout_evenv
      layout: even-vertical
      panes:
        - $1
snippet layout_mainh
      layout: main-horizontal
      panes:
        - $1
snippet layout_evenh
      layout: even-horizontal
      panes:
        - $1

