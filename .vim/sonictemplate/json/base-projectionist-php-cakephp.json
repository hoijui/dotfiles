{
	"Gemfile": { "type" : "configs"},
	"Gruntfile.coffee": { "type" : "configs"},
	"Plugin/*.php": { "type" : "plugins"},
	"Vendor/*.php": { "type" : "vendors"},
	"composer.json": { "type" : "configs"},
	"app/Config/*.php": { "type" : "configs"},
    "app/Console/Command/Shell/*.php": { "type" : "shells"},
    "app/Console/Command/Shell/Task/*.php": { "type" : "tasks"},
	"app/Controller/*.php": { "type" : "controllers"},
	"app/Controller/Component/*.php": { "type" : "components"},
	"app/Model/*.php": { "type" : "model"},
	"app/Model/Behavior/*.php": { "type" : "behavior"},
	"app/Routing/*.php": { "type" : "routes"},
	"app/Test/Case/*.php": { "type" : "tests"},
	"app/Test/Fixture/*.php": { "type" : "fixtures"},
	"app/View/Helper/*.php": { "type" : "helpers"},
	"app/View/*.ctp": { "type" : "views"},
	"app/webroot/js/*.js": { "type" : "javascripts"},
	"app/webroot/css/*.css": { "type" : "csses"},
    "app/Locale/*/LC_MESSAGES/*.po": { "type" : "locales"},
	"app/Lib/*.php": { "type" : "libs"},
	"app/Plugin/*.php": { "type" : "plugins"},
	"app/Vendor/*.php": { "type" : "vendors"},
	"README.md": { "type": "doc" }
}
