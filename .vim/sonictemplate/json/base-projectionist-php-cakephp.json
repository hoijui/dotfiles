{
	"Gemfile": { "type" : "configs"},
	"Gruntfile.coffee": { "type" : "configs"},
	"composer.json": { "type" : "configs"},
	"app/Config/*.php": { "type" : "configs"},
    "app/Console/Command/*.php": { "type" : "shells", "alternate": "app/Test/Case/Console/Command/{}.php"},
    "app/Console/Command/Task/*.php": { "type" : "tasks", "alternate": "app/Test/Case/Console/Command/Task/{}.php"},
	"app/Controller/*.php": { "type" : "controllers", "alternate": "app/Test/Case/Controller/{}.php"},
	"app/Controller/Component/*.php": { "type" : "components", "alternate": "app/Test/Case/Controller/Component/{}.php"},
	"app/Model/*.php": { "type" : "models", "alternate": "app/Test/Case/Model/{}.php"},
	"app/Model/Behavior/*.php": { "type" : "behavior", "alternate": "app/Test/Case/Model/Behavior/{}.php"},
	"app/Routing/*.php": { "type" : "routes"},
	"app/Test/Case/*Test.php": { "type" : "tests", "alternate": "app/{}.php"},
	"app/Test/Fixture/*Fixture.php": { "type" : "fixtures", "alternate": "app/Test/Case/Model/{}.php"},
	"app/View/Helper/*.php": { "type" : "helpers", "alternate": "app/Test/Case/View/Helper/{}.php"},
	"app/View/*.ctp": { "type" : "views"},
    "app/View/Elements/*.ctp": { "type" : "elements"},
    "app/View/Layouts/*.ctp": { "type" : "layouts"},
	"app/webroot/js/*.js": { "type" : "scripts"},
	"app/webroot/css/*.css": { "type" : "styles"},
    "app/Locale/*/LC_MESSAGES/*.po": { "type" : "locales"},
	"app/Lib/*.php": { "type" : "libs", "alternate": "app/Test/Case/Lib/{}.php"},
	"app/Plugin/*.php": { "type" : "plugins"},
	"app/Vendor/*.php": { "type" : "vendors"},
	"Plugin/*.php": { "type" : "plugins"},
	"Vendor/*.php": { "type" : "vendors"},
	"Vendor/pear-pear.cakephp.org/CakePHP/Cake/*.php": {"type":"core"},
	"README.md": { "type": "doc" }
}
