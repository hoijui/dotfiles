{
  "composer.json": { "type" : "config"},
  "bootstrap/*.php": { "type" : "bootstrap"},
  "config/*.php": { "type" : "config"},
  "app/Http/Controllers/*.php": { "type" : "controllers", "alternate": "tests/App/Http/Controllers/{}Test.php"},
  "app/Http/Middleware/*.php": { "type" : "middleware", "alternate": "test/App/Http/Middleware/{}Test.php"},
  "app/Http/Kernel.php": { "type" : "middleware", "alternate": "test/App/Http/KernelTest.php"},
  "app/Http/Requests/*.php": { "type" : "requests", "alternate": "test/App/Http/Requests/{}Test.php"},
  "app/Models/*.php": { "type" : "models", "alternate": "test/App/Models/{}Test.php"},
  "app/Scopes/*.php": { "type" : "scopes", "alternate": "tests/App/Scopes/{}Test.php"},
  "database/factories/*.php": { "type" : "factories"},
  "database/migrations/*.php": { "type" : "migrations"},
  "database/seeds/*.php": { "type" : "seeds"},
  "tests/TestCase.php": { "type" : "tests"},
  "tests/*Test.php": { "type" : "tests", "alternate": "app/App/{}.php"},
  "resources/views/*.php": { "type" : "views"},
  "resources/lang/*.php": { "type" : "lang"},
  "resources/assets/*": { "type" : "assets"},
  "public/*.js": { "type" : "scripts"},
  "public/*.css": { "type" : "styles"},
  "vendor/*.php": { "type" : "vendors"},
  "vendor/laravel/*.php": {"type":"core"},
  "vendor/illuminate/*.php": {"type":"core"},
  "app/Jobs/*.php": { "type" : "jobs", "alternate": "tests/App/Jobs/{}Test.php"},
  "app/Events/*.php": { "type" : "events", "alternate": "test/App/Events/{}Test.php"},
  "app/Listeners/*.php": { "type" : "listeners", "alternate": "tests/App/Listeners/{}Test.php"},
  "app/Handlers/*.php": { "type" : "handlers", "alternate": "tests/App/Handlers/{}Test.php"},
  "app/Policies/*.php": { "type" : "policies", "alternate": "tests/App/Policies/{}Test.php"},
  "app/Exceptions/*.php": { "type" : "exceptions", "alternate": "test/App/Exceptions/{}Test.php"},
  "app/Providers/*.php": { "type" : "providers", "alternate": "test/App/Providers/{}Test.php"},
  "app/Traits/*.php": { "type" : "traits", "alternate": "test/App/Traits/{}Test.php"},
  "app/Console/Kernel.php": { "type" : "commands", "alternate": "tests/App/Console/KernelTest.php"},
  "app/Console/Commands/*.php": { "type" : "commands", "alternate": "tests/App/Console/Commands/{}Test.php"},
  "routes/*.php": { "type" : "routes", "alternate": "tests/{}Test.php"},
  "app/Http/routes.php": { "type" : "routes", "alternate": "tests/App/Http/{}Test.php"},
  "app/Libs/*.php": { "type" : "libs", "alternate": "tests/App/Libs/{}.php"},
  "README.md": { "type": "doc" }
}


