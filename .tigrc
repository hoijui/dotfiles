# options {{{1
set show-date = local
set show-refs = yes
set show-rev-graph = yes
set line-graphics = yes

# binds {{{1
bind generic g move-first-line
bind generic G move-last-line
bind generic ^f move-page-down
bind generic ^b move-page-up
bind generic ^w view-next

bind diff ^w view-next

# http://hiroki.jp/2012/05/30/4182/
bind diff ; view-next
bind diff l scroll-right
bind diff h scroll-left

bind main Y !@git pbcopy %(commit)


# __END__ {{{1
